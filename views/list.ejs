<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>main</title>
</head>
<body>
    <h2>MAIN</h2>
    <hr>

    <table class="list">
        <thead>
          <tr>
            <th>boardid</th>
            <th>title</th>
            <th>id</th>
          </tr>
        </thead>
        <tbody>
          <% items.forEach(function(item) { %>
            <tr>
              <td id="boardid" name="boardid"><%= item.boardid %></td>
              <td><a href='/read?boardid=<%= item.boardid %>'><%= item.title %></a></td>
              <td><%= item.user_id %></td>
            </tr>
          <% }); %>
        </tbody>
    </table>

    <button id="createBtn" onclick="location.href='/create.html'">create</button>
    <button id="logoutBtn" onclick="location.href='/login.html'">logout</button>
</body>
</html>